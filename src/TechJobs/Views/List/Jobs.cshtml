<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>
<table class="job-listing">

    @foreach (Dictionary<string, string> item in ViewBag.jobs)
    {
        @foreach (KeyValuePair<string, string> entry in item)
        {
            <tr>
                <td><strong>@entry.Key</strong></td>
                <td>@entry.Value</td>
            </tr>
        }

    }
</table>
    }
